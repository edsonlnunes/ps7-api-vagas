{
  "name": "ps7-api-vagas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:up": "typeorm-ts-node-commonjs migration:run -d ./src/main/database/database-connection.ts",
    "typeorm:down": "typeorm-ts-node-commonjs migration:revert -d ./src/main/database/database-connection.ts",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/main/database/database-connection.ts",
    "test:u": "jest --silent --noStackTrace --runInBand -c ./jest.config.u.ts",
    "test:u:verbose": "jest --verbose --runInBand --detectOpenHandles -c ./jest.config.u.ts",
    "test:u:coverage": "jest --runInBand --coverage -c ./jest.config.u.ts",
    "test:u:watch": "jest --silent --noStackTrace --runInBand --watchAll -c ./jest.config.u.ts",
    "migration:test": "set NODE_ENV=test && typeorm-ts-node-commonjs migration:run -d ./src/main/database/database-connection.ts",
    "test:setup": "rimraf test.sqlite && npm run migration:test",
    "test:i": "set NODE_ENV=test && npm run test:setup && jest --silent --noStackTrace --runInBand -c ./jest.config.i.ts",
    "test:i:v": "set NODE_ENV=test && npm run test:setup && jest --verbose --runInBand -c ./jest.config.i.ts",
    "test:i:c": "set NODE_ENV=test && npm run test:setup && jest --silent --coverage --noStackTrace --runInBand -c ./jest.config.i.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edsonlnunes/ps7-api-vagas.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edsonlnunes/ps7-api-vagas/issues"
  },
  "homepage": "https://github.com/edsonlnunes/ps7-api-vagas#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.1.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  }
}
