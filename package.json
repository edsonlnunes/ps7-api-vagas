  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:up": "typeorm-ts-node-commonjs migration:run -d ./src/main/database/database-connection.ts",
    "typeorm:down": "typeorm-ts-node-commonjs migration:revert -d ./src/main/database/database-connection.ts",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/main/database/database-connection.ts",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll"
  },
  "repository": {
    "type": "git",
